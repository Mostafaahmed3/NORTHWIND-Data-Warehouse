TRUNCATE TABLE bronze.customer;
BULK INSERT bronze.customer
FROM 'D:\your Journy is possiple\NORTHWIND DW\dataset\customer.csv'
WITH (
    FIELDTERMINATOR = ',',
    ROWTERMINATOR = '0x0A',
    FIRSTROW = 2,
    TABLOCK
);



